pipeline {
    agent any
    stages { 
        stage('Build') {
            steps {
                echo 'In Build Environment'
            }
        }
        stage('Test') {
            steps {
                echo 'In Test Environment'
            }
        }
        stage('QA') {
            steps {
                echo 'In QA Environment'
            }
        }
        stage('Deploy') {
            steps {
                echo 'IDeployed to production'
            }
        }
    }
}
